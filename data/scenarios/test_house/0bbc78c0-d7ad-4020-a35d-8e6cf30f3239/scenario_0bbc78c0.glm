module tape;
module residential {
    implicit_enduses NONE;
};
module powerflow;
module climate;

#include "typical_light_yearly.glm";


clock {
    timezone PST+8PDT;
    starttime '2024-07-01 00:00:00';
    stoptime '2024-07-02 23:59:00';
}

object climate {
    tmyfile "WA-Yakima.tmy2";
};



object triplex_meter {
    name meter1;
    phases AS;
    nominal_voltage 120;
    
    
    
    object house {
        name house;
        heating_setpoint 68;
        cooling_setpoint 72;
        system_mode OFF;
        cooling_system_type ELECTRIC;
        design_cooling_setpoint 75;
        design_cooling_capacity 3.5;
        heating_system_type GAS;
        design_heating_setpoint 70;
        design_heating_capacity 4;
        thermostat_deadband 2;
        floor_area 2000;
        number_of_stories 1;
        ceiling_height 8;
        envelope_UA 300;
        window_wall_ratio 0.15;
        number_of_doors 2;
        Rwall 13;
        Rwindows 3;
        Rroof 30;
        Rfloor 10;
        glazing_layers 2;
        
        
        
        
        
        object ZIPload {
            name indoor_lights;
            parent house;
            base_power yearly_lighting*0.5;
            power_pf 0.95;
            heatgain_fraction 1.0;
            impedance_fraction 0.0;
            current_fraction 0.0;
            power_fraction 1.0;
            is_240 FALSE;
        };
        
        
        
        
        
        object ZIPload {
            name washing_machine;
            parent house;
            
            object player {
                property base_power;
                file "washing_machine_consumption.csv";
                loop 1;
            };
            
            heatgain_fraction 0.05;
            power_pf 0.95;
            impedance_fraction 0.1;
            current_fraction 0.0;
            power_fraction 0.9;
            is_240 True;
        };
        
        
        
        object multi_recorder {
            property house:total_load;
            file "results_0bbc78c0.csv";
            interval 60;
            limit 1440;
        };
    };
}